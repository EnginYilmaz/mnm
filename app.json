{
  "expo": {
    "name": "massagenearme",
    "description": "Masseuse finder application.",
    "slug": "masseuse",
    "privacy": "public",
    "sdkVersion": "33.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "versionCode": 32,
      "package": "com.masseusenearme.massage",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyCk8PGxIOYtUr0aQ8fErOSXRUYcPF9_XrU"
        }
      },
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "CAMERA",
        "MANAGE_DOCUMENTS",
        "READ_EXTERNAL_STORAGE",
        "NOTIFICATIONS"
      ]
    },
    "web": {
      "build": {
        "babel": {
          "include": ["native-base", "static-container"]
        }
      }
    }
  }
}
